{
  "name": "byrnedo/php-nats-streaming",
  "license": "MIT",
  "description": "PHP Client for nats-streaming-server.",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/vpmv/phpnats.git"
    }
  ],
  "require": {
    "repejota/nats": "0.8.8",
    "google/protobuf": "^3.11"
  },
  "require-dev": {
    "symfony/console": "^2.8@dev",
    "phpunit/phpunit": "5.3.*",
    "squizlabs/php_codesniffer": "~2.0",
    "google/protobuf": "^3.11",
    "satooshi/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "NatsStreaming\\": "src/NatsStreaming",
      "NatsStreamingProtos\\": "gen/NatsStreamingProtos"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
